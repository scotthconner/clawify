{
  "name": "clawify",
  "version": "1.0.0",
  "description": "TEE Bootstrap for OpenClaw with Crypto Wallet Access",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:docker": "docker build -t clawify . && docker run --rm -it --env-file .env clawify",
    "docker:build": "docker build -t clawify .",
    "docker:run": "docker run --rm -it --env-file .env clawify",
    "docker:shell": "docker run --rm -it --env-file .env clawify /bin/sh",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "tee",
    "openclaw",
    "crypto",
    "wallet",
    "eigenx"
  ]
}
